# CloudBase Environment Configuration
# Copy this file to .env.local and fill in your CloudBase credentials

# CloudBase Environment ID
# Find this in CloudBase Console > Overview > Environment Settings
VITE_CLOUDBASE_ENV_ID=your-env-id-here

# CloudBase Region (e.g., ap-shanghai, ap-guangzhou, ap-beijing)
VITE_CLOUDBASE_REGION=ap-shanghai

# Application Configuration
VITE_APP_NAME=ADV Moto Hub
VITE_APP_VERSION=1.0.0

# Map Configuration (optional - for custom map tiles)
VITE_MAP_TILE_URL=https://tile.openstreetmap.org/{z}/{x}/{y}.png
VITE_MAP_ATTRIBUTION='&copy; OpenStreetMap contributors'
